<picture>
    <source srcset="{{img_url feature_image size="s"}}.webp"  media="(max-width: 300px)"  type="image/webp">
    <source srcset="{{img_url feature_image size="m"}}.webp"  media="(max-width: 600px)"  type="image/webp">
    <source srcset="{{img_url feature_image size="l"}}.webp"  media="(max-width: 1000px)" type="image/webp">
    <source srcset="{{img_url feature_image size="xl"}}.webp" type="image/webp">
    <img class="img-fluid" src="{{img_url feature_image}}" alt="Feature Image">
</picture>
